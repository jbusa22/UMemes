{
  "name": "umemes",
  "version": "1.0.0",
  "description": "michigan meme generator",
  "main": "/umemes-server/server.js",
  "scripts": {
    "start": "node server",
    "build": "cd umemes-client/ && npm install && npm run build",
    "unpack": "npm install && cd umemes-client/ && npm install && npm run build",
    "client": "cd umemes-client/ && npm run start",
    "local": "concurrently \"npm run client\" \"npm run start\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbusa22/UMemes.git"
  },
  "author": "Jack Busa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jbusa22/UMemes/issues"
  },
  "homepage": "https://github.com/jbusa22/UMemes#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "flickr-sdk": "^3.9.0",
    "jimp": "^0.10.2",
    "nodemon": "^2.0.3",
    "path": "^0.12.7"
  }
}
